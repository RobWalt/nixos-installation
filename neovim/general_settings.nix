{}:
{
  content = ''
    set nocompatible
    set showmatch
    set ignorecase
    set hlsearch
    set incsearch
    set background=dark
    set clipboard=unnamedplus
    set completeopt=menu,menuone,noselect
    set cursorline
    set hidden
    set inccommand=split
    set relativenumber
    set splitbelow splitright
    set title
    set wildmenu
    set cc=120
    set expandtab
    set shiftwidth=4
    set tabstop=4
    set softtabstop=4
    set autoindent
    filetype plugin indent on
  '';
}
